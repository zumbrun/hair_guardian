<h1>Login</h1>

<%= form_with(model: @user, url: sessions_path, local: true) do |f| %>
	<div class="field">
	 	<%= f.label :username %>
	 	<%= f.text_field :username %>
	</div>
 	</br>	
	<div class="field">
	 	<%= f.label :password %>
	 	<%= f.password_field :password %>
	</div>	
	</br>	
	<div class="actions">
		<%= f.submit "Login" %>
	</div>
<% end %>